{
	"name": "@labdigital/graphql-codegen-k6",
	"version": "0.0.5",
	"type": "module",
	"description": "GraphQL Code Generator plugin for generating a ready-to-use k6 compatible SDK based on GraphQL operations",
	"repository": {
		"type": "git",
		"url": "https://github.com/labd/graphql-codegen-plugins.git",
		"directory": "packages/k6"
	},
	"license": "MIT",
	"engines": {
		"node": ">= 16.0.0"
	},
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"lint": "biome ci",
		"build": "tsup",
		"publish:ci": "pnpm build && pnpm changeset publish",
		"test:ci": "vitest run"
	},
	"dependencies": {
		"@graphql-codegen/plugin-helpers": "^5.0.4",
		"@graphql-codegen/visitor-plugin-common": "5.8.0",
		"auto-bind": "5.0.1"
	},
	"devDependencies": {
		"tslib": "2.8.1",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vitest": "3.1.2"
	},
	"peerDependencies": {
		"graphql": "16.x",
		"graphql-tag": "^2.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
