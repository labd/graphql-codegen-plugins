{
	"name": "@labdigital/graphql-codegen-k6",
	"version": "0.0.3",
	"type": "module",
	"description": "GraphQL Code Generator plugin for generating a ready-to-use k6 compatible SDK based on GraphQL operations",
	"repository": {
		"type": "git",
		"url": "https://github.com/labd/graphql-codegen-plugins.git",
		"directory": "packages/k6"
	},
	"license": "MIT",
	"engines": {
		"node": ">= 16.0.0"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"exports": {
		".": {
			"require": {
				"default": "./dist/index.cjs"
			},
			"import": {
				"default": "./dist/index.js"
			},
			"default": {
				"default": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"lint": "biome ci",
		"build": "tsup",
		"publish:ci": "pnpm build && pnpm changeset publish",
		"test:ci": "vitest run"
	},
	"peerDependencies": {
		"graphql": "^16.9.0",
		"graphql-tag": "^2.0.0"
	},
	"dependencies": {
		"@graphql-codegen/plugin-helpers": "^5.0.4",
		"@graphql-codegen/visitor-plugin-common": "2.13.1",
		"auto-bind": "~4.0.0"
	},
	"devDependencies": {
		"tslib": "~2.6.3",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vitest": "^2.1.6"
	},
	"publishConfig": {
		"access": "public"
	}
}
